[Unit]
Description=jackett
After=network.target

[Service]
Type=forking
KillMode=process
User=%I
ExecStart=/usr/bin/screen -f -a -d -m -S jackett mono /home/%I/Jackett/JackettConsole.exe
ExecStop=-/bin/kill -HUP
WorkingDirectory=/home/%I/
Restart=on-failure
RestartSec=2
TimeoutStopSec=5
[Install]
WantedBy=multi-user.target
